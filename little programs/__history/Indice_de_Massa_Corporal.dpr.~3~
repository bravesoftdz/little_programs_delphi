program Indice_de_Massa_Corporal;

uses
  Vcl.Forms,
  uFrmIndiceMassaCorporal in 'uFrmIndiceMassaCorporal.pas' {FrmIndiceMassaCorportal},
  uFrmRequisitos in 'uFrmRequisitos.pas' {FrmRequisitos};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFrmIndiceMassaCorportal, FrmIndiceMassaCorportal);
  Application.CreateForm(TFrmRequisitos, FrmRequisitos);
  Application.Run;
end.
